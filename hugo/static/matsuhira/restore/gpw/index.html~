<!-- begin chatx_cond_matsupla(Whocares使用領域) -->
<script type="text/javascript">

//<![CDATA[
function _chatxcond_onload_matsupla() {
  try {
    var e = document.createElement("script");
    var cs = document.characterSet? document.characterSet :
            document.charset? document.charset : "UTF-8";

    e.setAttribute("type", "text/javascript");
    e.setAttribute("charset", cs);
    e.setAttribute("src", "http://chatx.whocares.jp/c/cond.jsp?r=matsupla&ver=1&cs=" + encodeURIComponent(cs));
    document.getElementsByTagName("head")[0].appendChild(e);
  }
  catch(ex){ _chatxcond_error_matsupla(ex.message? ex.message : ex.name); }
}
function _chatxcond_error_matsupla(s) {
  var e = document.getElementById("chatxcond_matsupla");
  if (e) {
    e.innerHTML = s.replace(/[<>"&]/g, "");
    e.style.color = "#ff3333";
  }
}
document.write("<div id='chatxcond_matsupla'>Loading...</div>");
if (window.addEventListener) { window.addEventListener("load", _chatxcond_onload_matsupla, false); }
else if (window.attachEvent) { window.attachEvent("onload", _chatxcond_onload_matsupla); }
else if (! window.onload) { window.onload = _chatxcond_onload_matsupla; }
else { _chatxcond_error_matsupla("error(1)"); }
//]]>
</script>
<noscript><p>JavaScriptが無効です。</p></noscript>
<!-- end chatx_cond_matsupla -->

